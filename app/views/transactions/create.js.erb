<% if @transaction.errors.blank? %>
jQuery("#viewacctran").replaceWith("<%= escape_javascript(render(
    :partial => 'transactions/tran',
    :object => Transaction.view_transactions(session[:user_id],@transaction.account.id,1)))%>");

jQuery("#tranact").replaceWith("<%= escape_javascript(render(
    :partial => 'transactions/activitytransactions',
    :object => Comment.joins("JOIN transactions t ON comments.commentable_id = t.id").where("commentable_type = 'Transaction'").select("comments.*").order("created_at DESC"), :locals =>  {:type => 'transactions', :limit => 15}))%>");

jQuery("#newacctran").slideToggle();
jQuery("#newtranbtn").text = 'New Transaction';
<% end %>
$('.content').before('<%= escape_javascript(render(:partial => 'common/flashdisplay', :locals => {:flash => flash} ))%>');
jQuery("#transactions_save").removeAttr('disabled').val("Save");
